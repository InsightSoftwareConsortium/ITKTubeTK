<?xml version="1.0" encoding="UTF-8"?>
<executable>
  <category>TubeTK</category>
  <title>Compute Pseudo-Probability of Tube Points (TubeTK)</title>
  <description>Takes all points of the input tubes, maps them into the space of the mean Euclidean distance image, and outputs the distance values at those points.</description>
  <version>0.1.0.$Revision: 2104 $(alpha)</version>
  <documentation-url>http://public.kitware.com/Wiki/TubeTK</documentation-url>
  <license>Apache 2.0</license>
  <contributor>Stephen R. Aylward, Roland Kwitt (Kitware)</contributor>
  <acknowledgements>This work is part of the TubeTK project at Kitware.</acknowledgements>
  <parameters>
    <label>IO</label>
    <description>Input/output parameters.</description>
    <file>
      <name>inTubeFile</name>
      <label>Input Tube File</label>
      <index>0</index>
      <description>Input tube file.</description>
    </file>
    <file>
      <name>inMeanImageFile</name>
      <label>Mean Euclidean Distance Map</label>
      <index>1</index>
      <description>Mean Euclidean distance map.</description>
    </file>
    <file>
      <name>outFile</name>
      <label>Output File</label>
      <index>2</index>
      <description>Output file with point "probabilities."</description>
    </file>
  </parameters>
</executable>
